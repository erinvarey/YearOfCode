{
  "manifest_version": 2,
 
  "name": "Sombrero Cat",
  "version": "0.1.0",
  "description": "Facebook now has cats",

"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Cats"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "content_scripts": [{
    //"css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["*://*/*"]
  }]
 
}