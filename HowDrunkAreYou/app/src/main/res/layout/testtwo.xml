<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/precisionTest"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/CircleThree"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp"
        android:text='""'
        android:visibility="invisible"
        android:layout_below="@+id/CircleTwo"
        android:layout_toRightOf="@+id/CircleOne"
        android:layout_toEndOf="@+id/CircleOne"
        android:layout_marginTop="40dp" />

    <Button
        android:id="@+id/CircleOne"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="39dp"
        android:text='""'
        android:visibility="invisible" />

    <Button
        android:id="@+id/CircleTwo"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:layout_marginTop="17dp"
        android:text='""'
        android:visibility="invisible"
        android:layout_below="@+id/CircleOne"
        android:layout_alignRight="@+id/CircleThree"
        android:layout_alignEnd="@+id/CircleThree"
        android:layout_marginRight="24dp"
        android:layout_marginEnd="24dp" />
</RelativeLayout>